<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/apps_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >


    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/app_icw"
        android:layout_width="480dp"
        android:layout_height="260dp"
        android:orientation="vertical"
        android:visibility="gone"
        android:layout_weight="1"
        >
        <TextView
            android:id="@+id/icw_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="警告！"
            android:padding="8dp"
            android:textColor="#FFC125"
            android:textSize="24sp" />
        <ImageView
            android:id="@+id/icw_img"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@mipmap/ic_launcher"
            android:scaleType="fitXY"
            />
    </LinearLayout>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/app_tlosa"
        android:layout_width="480dp"
        android:layout_height="260dp"
        android:orientation="vertical"
        android:visibility="gone"
        android:layout_weight="1"
        >
        <TextView
            android:id="@+id/tlosa_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="8dp"
            android:text="提示信息"
            android:textColor="#FFC125"
            android:textSize="24sp" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">
            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/tlosa_left"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="1"
                android:visibility="gone"
                >

                <ImageView
                        android:id="@+id/tlosa_left_img"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:src="@drawable/left"
                        android:layout_weight="1"
                        />
                <TextView
                        android:id="@+id/tlosa_left_value"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:text="34s"
                        android:textSize="24sp"
                        android:gravity="center"
                        />
                <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                        android:id="@+id/tlosa_left_maxspeed"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:orientation="horizontal"
                        android:layout_weight="1"
                        >
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="Max:"
                            android:textSize="24sp"
                            android:gravity="center"
                            android:textColor="#F70044"
                            />
                        <TextView
                            android:id="@+id/tlosa_left_max_value"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="100"
                            android:textSize="24sp"
                            android:gravity="center"
                            />
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="KM/H"
                            android:textSize="18sp"
                            android:gravity="center"
                            />
                    </LinearLayout>

                <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                        android:id="@+id/tlosa_left_minspeed"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:orientation="horizontal"
                        android:layout_weight="1"
                        >
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="Min:"
                            android:textSize="24sp"
                            android:gravity="center"
                            android:textColor="#11CD86"
                            />
                        <TextView
                            android:id="@+id/tlosa_left_min_value"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:text="20"
                            android:textSize="24sp"
                            />
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="KM/H"
                            android:textSize="18sp"
                            android:gravity="center"
                            />
                    </LinearLayout>

            </LinearLayout>
            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/tlosa_head"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="1"
                android:visibility="gone"
                >

                <ImageView
                        android:id="@+id/tlosa_head_img"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:src="@drawable/head"
                        android:layout_weight="1"
                        />
                <TextView
                        android:id="@+id/tlosa_head_value"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                    android:layout_weight="1"

                        android:text="5s"
                        android:textSize="24sp"
                        android:gravity="center"
                        />
                <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                        android:id="@+id/tlosa_head_maxspeed"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:orientation="horizontal"
                        android:layout_weight="1"
                        >
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="Max:"
                            android:textSize="24sp"
                            android:gravity="center"
                            android:textColor="#F70044"
                            />
                        <TextView
                            android:id="@+id/tlosa_head_max_value"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="100"
                            android:textSize="24sp"
                            android:gravity="center"
                            />
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="KM/H"
                            android:textSize="18sp"
                            android:gravity="center"
                            />


                        />
                    </LinearLayout>
                <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                        android:id="@+id/tlosa_head_minspeed"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:orientation="horizontal"
                        android:layout_weight="1"
                        >
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="Min:"
                            android:textSize="24sp"
                            android:gravity="center"
                            android:textColor="#11CD86"
                            />
                        <TextView
                            android:id="@+id/tlosa_head_min_value"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:text="20"
                            android:textSize="24sp"
                            />
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="KM/H"
                            android:textSize="18sp"
                            android:gravity="center"
                            />
                    </LinearLayout>
            </LinearLayout>
            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/tlosa_right"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="1"
                android:visibility="gone"
                >

                <ImageView
                        android:id="@+id/tlosa_right_img"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:src="@drawable/right"
                        android:layout_weight="1"
                        />
                <TextView
                        android:id="@+id/tlosa_right_value"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:text="10s"
                        android:textSize="24sp"
                        android:gravity="center"
                        />
                <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                        android:id="@+id/tlosa_right_maxspeed"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:orientation="horizontal"
                        android:layout_weight="1"
                        >
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="Max:"
                            android:textSize="24sp"
                            android:gravity="center"
                            android:textColor="#F70044"
                            />
                        <TextView
                            android:id="@+id/tlosa_right_max_value"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="100"
                            android:textSize="24sp"
                            android:gravity="center"
                            />
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="KM/H"
                            android:textSize="18sp"
                            android:gravity="center"
                            />


                        />
                    </LinearLayout>
                <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                        android:id="@+id/tlosa_right_minspeed"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:orientation="horizontal"
                        android:layout_weight="1"
                        >
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="Min:"
                            android:textSize="24sp"
                            android:gravity="center"
                            android:textColor="#11CD86"
                            />
                        <TextView
                            android:id="@+id/tlosa_right_min_value"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:text="20"
                            android:textSize="24sp"
                            />
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="KM/H"
                            android:textSize="18sp"
                            android:gravity="center"
                            />
                    </LinearLayout>



            </LinearLayout>

        </LinearLayout>

    </LinearLayout>




</LinearLayout>