<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/apps_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    >


    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/app_icw"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="gone"
        android:layout_weight="1"
        >
        <TextView
            android:id="@+id/icw_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="10dp"
            android:text="警告！"
            android:textColor="#FFC125"
            android:textSize="24sp" />
        <ImageView
            android:id="@+id/icw_img"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/ic_launcher"
            />
    </LinearLayout>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/app_tlosa"
        android:layout_width="480dp"
        android:layout_height="240dp"
        android:orientation="vertical"
        android:visibility="gone"
        android:layout_weight="1"
        >
        <TextView
            android:id="@+id/tlosa_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="10dp"
            android:text="警告！"
            android:textColor="#FFC125"
            android:textSize="24sp" />
        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/tlosa_light"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:orientation="horizontal"
            android:layout_weight="1"
            >

            <ImageView
                android:id="@+id/tlosa_left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/left"
                android:layout_weight="1"

                />
            <ImageView
                android:id="@+id/tlosa_head"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/head"
                android:layout_weight="1"
                />
            <ImageView
                android:id="@+id/tlosa_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/right"
                android:layout_weight="1"
                />
        </LinearLayout>

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/tlosa_second"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:orientation="horizontal"
            android:layout_weight="1"
            >

            <TextView
                android:id="@+id/tlosa_left_s"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="1s"
                android:textSize="24sp"
                android:gravity="center"
                />
            <TextView
                android:id="@+id/tlosa_head_s"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="1s"
                android:textSize="24sp"
                android:gravity="center"
                />
            <TextView
                android:id="@+id/tlosa_right_s"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="1s"
                android:textSize="24sp"
                android:gravity="center"
                />
        </LinearLayout>
        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/tlosa_speed"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:orientation="horizontal"
            android:layout_weight="1"
            >

            <TextView
                android:id="@+id/tlosa_max"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Max:"
                android:textSize="24sp"
                android:gravity="right"
                />
            <TextView
                android:id="@+id/tlosa_max_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="100s"
                android:textSize="24sp"
                android:gravity="center"
                />
            <TextView
                android:id="@+id/tlosa_min"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Min:"
                android:textSize="24sp"
                android:gravity="right"
                />
            <TextView
                android:id="@+id/tlosa_min_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:layout_weight="1"
                android:text="20s"
                android:textSize="24sp"
                />
        </LinearLayout>
    </LinearLayout>




</LinearLayout>